    data jsonb
);


ALTER TABLE public."CheckIn" OWNER TO postgres;

--
-- Name: CheckIn_id_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public."CheckIn_id_seq"
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public."CheckIn_id_seq" OWNER TO postgres;

