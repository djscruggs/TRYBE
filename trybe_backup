--
-- Name: Challenge; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public."Challenge" (
    id integer NOT NULL,
    "createdAt" timestamp(3) without time zone DEFAULT CURRENT_TIMESTAMP NOT NULL,
    "updatedAt" timestamp(3) without time zone NOT NULL,
    "publishAt" timestamp(3) without time zone DEFAULT CURRENT_TIMESTAMP NOT NULL,
    name text NOT NULL,
    "userId" integer NOT NULL,
    color text,
    description text,
    "endAt" timestamp(3) without time zone,
    icon text,
    reminders boolean DEFAULT false NOT NULL,
    "startAt" timestamp(3) without time zone DEFAULT CURRENT_TIMESTAMP,
    "syncCalendar" boolean DEFAULT false NOT NULL,
    frequency public."Frequency" DEFAULT 'DAILY'::public."Frequency" NOT NULL,
    mission text,
    public boolean DEFAULT false NOT NULL,
    video text,
    "commentCount" integer DEFAULT 0 NOT NULL,
    "likeCount" integer DEFAULT 0 NOT NULL,
    "coverPhotoMeta" jsonb,
    "videoMeta" jsonb,
    "numDays" integer,
    type public."ChallengeType" DEFAULT 'SCHEDULED'::public."ChallengeType" NOT NULL,
    status public."ChallengeStatus" DEFAULT 'DRAFT'::public."ChallengeStatus" NOT NULL
);


ALTER TABLE public."Challenge" OWNER TO postgres;

--
-- Name: Challenge_id_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public."Challenge_id_seq"
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public."Challenge_id_seq" OWNER TO postgres;

--
-- Name: Challenge_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public."Challenge_id_seq" OWNED BY public."Challenge".id;


