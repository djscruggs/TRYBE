    id integer NOT NULL,
    "createdAt" timestamp(3) without time zone DEFAULT CURRENT_TIMESTAMP NOT NULL,
    "userId" integer NOT NULL,
    "postId" integer,
    "challengeId" integer,
    "commentId" integer,
    "noteId" integer,
    "threadId" integer,
    "checkinId" integer
);


